package com.kjh;

public class MainClass {
	//MainClass : 프로젝트 전체를 컨트롤하는 역할, 여러개로 나누어져있는 클래스들을 통합 관리 할 예정
	//Controller 역할  
	public static void main(String[] args) {
		//MemberClass 
		//("92001", "590327-1839240", "이태규", 920, "교수", "1997")
		DataClass dc = new DataClass();
		for(int i = 0;i<dc.members.length;i++) {
			System.out.println(dc.members[i].pname);
			System.out.println(dc.members[i].cCode);
		}
	}
}