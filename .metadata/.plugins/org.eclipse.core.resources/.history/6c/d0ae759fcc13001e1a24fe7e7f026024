package com.kjh;
// 한 회원의 정보를 하나로 묶어주는 역할
// Model역할
public class MemberClass {
	String name  = null;      		//이름
	String joinDate = null;   		// 가입일
	String lastVisitDate = null; 	// 최종방문일
	int numVisit = 0;			//방문횟수
	String gender = null;      	//   성별
	String age = null;         	//	연령대
	
	boolean b = true;
	
	public MemberClass() {
		MethodClass.splitText();
		System.out.println("MemberClass 생성자 호출");
		
	}

}
