package com.kjh;

public class MainClass {
	//MainClass : 프로젝트 전체를 컨트롤하는 역할, 여러개로 나누어져있는 클래스들을 통합 관리 할 예정
	//Controller 역할  
	public static void main(String[] args) {
		//MemberClass 
		//("92001", "590327-1839240", "이태규", 920, "교수", "1997")
		MemberClass m = new MemberClass("92001", "590327-1839240", "이태규", 920, "교수", "1997");
		System.out.println(2023 - m.hireDate);
	}
}